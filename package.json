{
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "start": "next start",
    "test": "pnpm prettier:check && pnpm lint",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prisma:migrate:resolve": "dotenv -e .env.development.local -- npx prisma migrate resolve --applied \"migration\"",
    "prisma:migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e .env.development.local -- npx prisma generate",
    "postbuild": "echo 'Postbuild started' && curl -X GET https://next-js-starter-lyart.vercel.app/api/restore-timers && echo 'Postbuild completed'"
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@heroicons/react": "2.1.3",
    "@prisma/client": "5.15.1",
    "@vercel/postgres": "0.8.0",
    "bootstrap": "5.3.3",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "dinero.js": "2.0.0-alpha.10",
    "dotenv": "16.4.5",
    "dotenv-cli": "7.4.2",
    "mapbox-gl": "3.4.0",
    "mqtt": "5.7.3",
    "ms": "3.0.0-canary.1",
    "next": "14.3.0-canary.33",
    "react": "18.2.0",
    "react-bootstrap": "2.10.4",
    "react-dom": "18.2.0",
    "react-icons": "5.2.1",
    "react-modal": "3.16.1",
    "react-select": "5.8.0",
    "react-tabs": "6.0.2",
    "reactjs-popup": "2.0.6",
    "server-only": "0.0.1",
    "styled-components": "6.1.8",
    "use-count-up": "3.0.1",
    "vercel": "34.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.12",
    "@types/mqtt": "2.5.0",
    "@types/ms": "0.7.34",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@types/react-modal": "3.16.3",
    "@vercel/git-hooks": "1.0.0",
    "autoprefixer": "10.4.19",
    "daisyui": "4.12.2",
    "eslint": "9.0.0",
    "eslint-config-next": "14.2.2",
    "lint-staged": "15.2.2",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "prisma": "5.15.1",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  }
}
